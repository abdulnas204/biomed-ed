<Files ~ "\.(php)$">
	order allow,deny
	allow from all
</Files>
<Files ~ "\.(js)$">
	order allow,deny
	allow from all
</Files>
<Files ~ "\.(css)$">
	order allow,deny
	allow from all
</Files>
<Files ~ "\.(swf)$">
	order allow,deny
	allow from all
</Files>
<Files ~ "\.(png)$">
	order allow,deny
	allow from all
</Files>
<Files ~ "\.(htaccess)$">
	order deny,allow
	deny from all
</Files>

ErrorDocument 403 /biomed-ed/system/deny/index.php?error=403
ErrorDocument 404 /biomed-ed/system/deny/index.php?error=404